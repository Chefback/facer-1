    "@tensorflow/tfjs-node-gpu": "^4.1.0",
    "@vladmandic/face-api": "^1.7.6",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
